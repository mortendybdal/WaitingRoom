          <script src="bower_components/json3/lib/json3.min.js"></script>
          <![endif]

        .page(ng-controller="MainCtrl", ng-class="{'show': ui_type !== ''}")
            ng-include(src="'partials/topbar'", ng-show="ui_type === 'dashboard'")
            ng-include(src="'partials/sidebar'", ng-show="ui_type === 'dashboard'")
            ng-include(src="'partials/tablet/tablet_navigation'", ng-show="ui_type === 'tablet'")

            .content(ng-class="{'navigation-builder-visible': show_content_tree}")
                ng-include(src="'partials/builder/navigation-builder'", ng-show="ui_type === 'dashboard'")
                .content-view(ng-view, class="{{page_class}}")

            .connection_status(ng-class="{'is_offline': !online}")
                span.connection_message {{d.GENERAL_TabletIsOffline}}

